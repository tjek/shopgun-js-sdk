{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": ["> 3%", "IE 11"],
                    "node": 10
                },
                "corejs": 3,
                "useBuiltIns": "usage",
                "exclude": ["transform-typeof-symbol"]
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-runtime", {"corejs": 3}]
    ]
}
