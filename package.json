{
    "version": "3.4.0",
    "description": "ShopGun SDK for JavaScript.",
    "main": "dist/shopgun-sdk/sgn-sdk.cjs.js",
    "browser": "dist/shopgun-sdk/sgn-sdk.js",
    "module": "dist/shopgun-sdk/sgn-sdk.es.js",
    "jsnext:main": "dist/shopgun-sdk/sgn-sdk.es.js",
    "bundlesize": [
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.js",
            "maxSize": "150 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.min.js",
            "maxSize": "75 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.cjs.js",
            "maxSize": "30 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.es.js",
            "maxSize": "30 kB"
        },
        {
            "path": "./dist/kits/events/index.js",
            "maxSize": "45 kB"
        },
        {
            "path": "./dist/kits/events/index.min.js",
            "maxSize": "25 kB"
        },
        {
            "path": "./dist/kits/events/index.cjs.js",
            "maxSize": "10 kB"
        },
        {
            "path": "./dist/kits/events/index.es.js",
            "maxSize": "10 kB"
        }
    ],
    "scripts": {
        "test": "npm run test:lint && npm run test:ci && npm run test:size",
        "test:lint": "eslint lib __tests__",
        "test:ci": "jest --verbose",
        "test:size": "bundlesize",
        "build:js": "rollup -c --no-treeshake -m --environment NODE_ENV:production",
        "build:css": "stylus lib/stylus/sgn.styl -o dist/shopgun-sdk/sgn-sdk.css --include-css -m",
        "build:css-min": "stylus  --use ./lib/stylus/cli-variables.js --with '{ COMPRESS: true }' lib/stylus/sgn.styl -o dist/shopgun-sdk/sgn-sdk.min.css --include-css -m -c",
        "build": "run-p -l build:js build:css build:css-min",
        "dev:js": "rollup -c -m inline --no-treeshake --environment NODE_ENV:development --watch",
        "dev:css": "stylus lib/stylus/sgn.styl -o dist/sgn-sdk.css --include-css --sourcemap-inline --watch",
        "dev": "run-p -l dev:js dev:css serve",
        "serve": "serve",
        "prepublishOnly": "npm run test && npm run build",
        "publish-pages": "gh-pages -d examples -e examples && gh-pages -d dist -e dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shopgun/shopgun-js-sdk.git"
    },
    "author": "Morten Bo RÃ¸nsholdt <mr@shopgun.com> (https://shopgun.com/)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/shopgun/shopgun-js-sdk/issues"
    },
    "homepage": "https://github.com/shopgun/shopgun-js-sdk#readme",
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "@babel/runtime-corejs3": "^7.12.5",
        "core-js": "^3.8.2",
        "cross-fetch": "^3.0.6",
        "incito-browser": "^1.2.0",
        "md5": "^2.3.0",
        "microevent": "^1.0.0",
        "mustache": "^4.1.0",
        "sha256": "^0.2.0",
        "verso-browser": "^1.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/eslint-parser": "^7.12.1",
        "@babel/plugin-external-helpers": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "app-root-path": "^3.0.0",
        "aws-sdk": "^2.822.0",
        "bundlesize": "^0.18.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "gh-pages": "^3.1.0",
        "jest": "^26.6.3",
        "jest-cli": "^26.6.3",
        "jest-each": "^26.6.2",
        "jest-puppeteer-preset": "^4.4.0",
        "jest-test-matrix": "^1.2.0",
        "memfs": "^3.2.0",
        "memory-fs": "^0.5.0",
        "nib": "^1.1.2",
        "npm-run-all": "^4.1.5",
        "process": "^0.11.10",
        "puppeteer": "^5.5.0",
        "rollup": "^2.36.1",
        "rollup-plugin-generate-package-json": "^3.2.0",
        "rollup-plugin-terser": "^7.0.2",
        "serve": "^11.3.2",
        "stylus": "^0.54.8",
        "thenify": "^3.3.1",
        "webpack": "^5.11.1"
    }
}
