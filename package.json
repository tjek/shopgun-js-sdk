{
    "description": "Tjek SDK for JavaScript.",
    "browserslist": "supports css-grid and supports beacon",
    "main": "dist/shopgun-sdk/sgn-sdk.cjs.js",
    "browser": "dist/shopgun-sdk/sgn-sdk.js",
    "module": "dist/shopgun-sdk/sgn-sdk.es.js",
    "jsnext:main": "dist/shopgun-sdk/sgn-sdk.es.js",
    "bundlesize": [
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.js",
            "maxSize": "90 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.min.js",
            "maxSize": "55 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.cjs.js",
            "maxSize": "70 kB"
        },
        {
            "path": "./dist/shopgun-sdk/sgn-sdk.es.js",
            "maxSize": "70 kB"
        },
        {
            "path": "./dist/incito-browser/incito.js",
            "maxSize": "20 kB"
        },
        {
            "path": "./dist/incito-browser/incito.min.js",
            "maxSize": "15 kB"
        },
        {
            "path": "./dist/incito-browser/incito.cjs.js",
            "maxSize": "10 kB"
        },
        {
            "path": "./dist/incito-browser/incito.es.js",
            "maxSize": "10 kB"
        },
        {
            "path": "./dist/verso-browser/verso.js",
            "maxSize": "25 kB"
        },
        {
            "path": "./dist/verso-browser/verso.min.js",
            "maxSize": "15 kB"
        },
        {
            "path": "./dist/verso-browser/verso.cjs.js",
            "maxSize": "20 kB"
        },
        {
            "path": "./dist/verso-browser/verso.es.js",
            "maxSize": "20 kB"
        },
        {
            "path": "./dist/tjek-sdk/index.cjs.js",
            "maxSize": "45 kB"
        },
        {
            "path": "./dist/tjek-sdk/index.es.js",
            "maxSize": "45 kB"
        }
    ],
    "scripts": {
        "clean": "rimraf -r dist",
        "test": "run-p -l -c test:*",
        "test:lint": "eslint lib __tests__",
        "test:ci": "jest",
        "test:size": "bundlesize",
        "test:types": "tsc -noEmit",
        "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c -m --environment NODE_ENV:production",
        "dev:js": "rollup -c -m inline --environment NODE_ENV:development --watch",
        "dev": "run-p -l dev:js serve",
        "serve": "serve",
        "prepublishOnly": "npm run clean && npm run build && npm run test",
        "publish": "GOOD=1 node publish-npm.mjs",
        "publish-pages": "gh-pages -d examples -e examples && gh-pages -d dist -e dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tjek/tjek-js-sdk.git"
    },
    "author": "Morten Bo RÃ¸nsholdt <mr@tjek.com> (https://tjek.com)",
